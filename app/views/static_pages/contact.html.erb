<% provide(:title, "Contact") %>
<h1>Contact</h1>

<form>
   <div class="row mb-1">
    <for="validationServer01" class="col-sm-2 col-form-label">Subject:</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="validationServer01">
    </div>
  </div>
  <div class="row mb-1">
    <label for="validationServer02" class="col-sm-2 col-form-label">Name</label>
    <div class="col-sm-3">
      <input type="text" class="form-control" id="validationServer02">
    </div>
  </div>
  <div class="row mb-1">
    <label for="validationServer03" class="col-sm-2 col-form-label">Email</label>
    <div class="col-sm-3">
      <input type="email" class="form-control" id="validationServer03">
    </div>
  </div>
  <div class="row mb-1">
    <label for="validationServer04" class="col-sm-2 col-form-label">Phone</label>
    <div class="col-sm-3">
      <input type="text" class="form-control" id="validationServer04">
    </div>
  </div>
    <div class="mb-1">
      <label for="exampleFormControlTextarea1" class="form-label">Message:</label>
      <textarea class="form-control" id="exampleFormControlTextarea1" rows="3"></textarea>
    </div>
    <div class="form-check">
      <input class="form-check-input" type="checkbox" value="" id="flexCheckChecked" checked>
      <label class="form-check-label" for="flexCheckChecked">
        Checked checkbox
      </label>
    </div>
    <div class="col-3">
        <button class="btn btn-primary" type="submit">Submit form</button>
      </div>
  </form>
